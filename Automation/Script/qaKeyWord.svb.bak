'USEUNIT qaActions
'USEUNIT qaGlobalVar

Function Action(ActionKey,objSelector,Data,OverrideData)
	Select Case ActionKey
    Case "OPEN"
      Call qaFunction.OpenBrowser 
  	Case "KEYIN"
      Call qaFunction.EnterText(objSelector,Data)
    Case "CLICK"
      Call qaFunction.Click(objSelector)
    CASE "LISTITEM"   
      Call qaFunction.SelectOptionByValue(objSelector,Data)
    Case "WAIT"
      Call qaFunction.Wait(Data)
    Case "SYNC"
      Call qaFunction.Sync(objSelector)
    Case "REPORT"
      Call qaFunction.LogReport(objSelector,Data)
    Case "VERFYCOLUMN"
      Call qaFunction.VerifyColumn(objSelector,Data)
  End Select
End Function